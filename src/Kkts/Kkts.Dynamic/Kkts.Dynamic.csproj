<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Linh Le</Authors>
    <Company>KKTS Ltd.</Company>
    <Description>A lightweight and dynamic mapper, you don't need to declare any classes for View Model or Data Transfer Object, just declare property binding</Description>
    <Copyright>Copyright (C) 2016-2020 KKTS Ltd.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/linhnle/Kkts.Dynamic</PackageProjectUrl>
    <RepositoryUrl>https://github.com/linhnle/Kkts.Dynamic.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Mapper, DynamicMapper, AnonymousType</PackageTags>
    <Version>1.0.0-rc1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reflection.Emit" Version="4.7.0" />
  </ItemGroup>

</Project>
